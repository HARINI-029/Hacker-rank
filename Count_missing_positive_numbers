
int countmissingpositivenumbers ( int n , int *arr)
{
    int i =0;
    while (i<n){
        int correct = arr[i] - 1;
        
        if ( arr[i]> 0 && arr[i]<= n && arr[i]!= arr[correct] )
        {
        int temp = arr[i];
        arr[i] = arr[correct];
        arr[correct ] = temp;
        }
    
    else
    i++;
    }
    int count = 0;
    
    for (int i = 0 ; i < n;i++)
    {
        if (arr[i]!= i + 1)
        {
            count ++;
        }
    }
    return count ;
}
