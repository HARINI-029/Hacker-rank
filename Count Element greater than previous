int countResponseTimeRegressions(int responseTimes_count, int* responseTimes) {
   
   long long sum = responseTimes [0], count = 0;
   
   for (int i = 1 ;i<responseTimes_count;i++)
   {
    double avg = (double)sum / i;
    if (responseTimes[i]>avg)
    {
        count ++;
    }
    sum+= responseTimes[i];
   }
   return count;

}
