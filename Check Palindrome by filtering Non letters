bool isAlphabeticPalindrome(char* code) { 
   int start = 0, end = strlen (code) - 1;
   while (start<end)
   {
    if (!isalpha(code[start]))
    {
        start++;
        continue;   
    }
    if (!isalpha(code[end]))
    {
        end --;
        continue;
    }
   if (tolower(code[start]) != tolower(code[end])) {
            return false;
        }

    start++;
    end--;
   }
   return 1;
}
